@model List<Entity.Bookmark>

@for (int i = 0; i < Model.Count; i++)
{
    <div id="customWrapper">
        <label asp-for="@Model[i].ShortDescription"></label>
        <input name="Person.Hobbies[@i].Description" value="@Model[i].ShortDescription" />
        <span asp-validation-for="@Model[i].ShortDescription"></span>
    </div>
}